<% for (const category of STATS_CATEGORIES) { %>
<% const total = category === STATS_CATEGORY_TOTAL ?
     TOTAL_WORDS.ALL :
     TOTAL_WORDS[DIFFICULTY_GROUP[category]] %>
<% if (category === DIFFICULTY.WHY) { %>
<%- include('Progress', { category, total, onTitle: title => `${title}?` }) %>
<% } else { %>
<%- include('Progress', { category, total }) %>
<% } %>
<% } %>

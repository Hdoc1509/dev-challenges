<% if (!(locals.close ?? false)) { %>
<!-- NOTE: to be used internally by icon components -->
<svg
  viewBox="0 0 24 24"
  focusable="false"
  aria-hidden="true"
  ejs="<% if (locals.class != null) { %>"
  class="<%= locals.class %>"
  ejs="<% } %>"
  ejs="<% if (!(locals.filled ?? false)) { %>"
  fill="none"
  stroke="currentColor"
  stroke-width="2"
  stroke-linecap="round"
  stroke-linejoin="round"
  ejs="<% } else { %>"
  fill="currentColor"
  ejs="<% } %>"
>
  <!-- children -->
  <% } else { %>
  <!-- children -->
</svg>
<% } %>

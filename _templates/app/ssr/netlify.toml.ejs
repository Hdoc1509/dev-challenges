---
to: ssr/<%= name %>/netlify.toml
---

[build]
  command = 'pnpm --filter <%= name %> build'
  publish = 'ssr/<%= name %>/dist'
  ignore = 'bash ./scripts/docs-check.sh ssr/<%= name %>'

[functions]
  # generated by @astrojs/netlify on build
  directory = 'ssr/<%= name %>/.netlify/functions-internal'
